# WineDirect webservices
WINE_USERNAME=your_webservice_username
WINE_PASSWORD=your_webservice_password
WINE_REGION=us
WINE_VERSION=v3
WINE_INVENTORY_FILTER=OnlySKUsWithInventoryOn

# Order detail: GetOrderDetail is called per order when 1. With 6000+ orders this hits rate
# limits and can appear stuck. Set to 0 to skip (fewer line-item details) or use
# WINE_ORDER_DETAIL_MAX to cap how many details are fetched (e.g. 2000).
# WINE_FETCH_ORDER_DETAIL=0
# WINE_ORDER_DETAIL_MAX=2000

# Wait when rate limited instead of failing (recommended when WINE_FETCH_ORDER_DETAIL=1)
# WINE_RATE_LIMIT_WAIT=1

# App
FLASK_SECRET_KEY=change-this
ADMIN_USER=admin
ADMIN_PASSWORD=change-this
CACHE_DAYS=400
CACHE_HOUR=2
CACHE_MINUTE=15

# Flask
FLASK_ENV=production
